<h1>Add Image</h2>
<h2><%=@newurl%></h2>

<form action="<%=@newurl%>" enctype="multipart/form-data" method="post">
	<input type="file" name="file" required><br />
	<input type="text" name="caption" placeholder="Image Caption"/><br />
	<input type="text" name="author" placeholder="Your Name"/><br />
	
	<input type="submit" value="Submit" />
</form>
